{
  "name": "graphql-api-gateway",
  "description": "An GraphQL API gateway for DoSomething.org services.",
  "scripts": {
    "start": "nodemon index.js",
    "start:production": "node index.js",
    "test": "npm run lint && npm run format:ci",
    "lint": "eslint --ext .js src",
    "format:ci": "prettier-check {src,config}/**/*.js",
    "format": "prettier --write {src,config}/**/*.js"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.3.x"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DoSomething/graphql.git"
  },
  "author": "David Furnes <dfurnes@dosomething.org>",
  "license": "MIT",
  "@std/esm": "cjs",
  "eslintConfig": {
    "extends": "@dosomething/eslint-config",
    "plugins": [
      "prettier"
    ],
    "globals": {
      "fetch": false
    },
    "rules": {
      "prettier/prettier": "error",
      "id-length": [
        "error",
        {
          "exceptions": [
            "_"
          ]
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_queryType"
          ]
        }
      ],
      "space-unary-ops": "off",
      "arrow-parens": "off"
    }
  },
  "dependencies": {
    "@std/esm": "^0.16.0",
    "apollo-engine": "^0.6.0",
    "apollo-link": "^1.0.3",
    "apollo-link-http": "^1.2.0",
    "apollo-server-express": "^1.2.0",
    "async-file": "^2.0.2",
    "body-parser": "^1.18.2",
    "connect-redis": "^3.3.2",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "forcedomain": "^0.8.1",
    "graphql": "^0.11.7",
    "graphql-iso-date": "^3.3.0",
    "graphql-server-express": "^1.2.0",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.8.0",
    "graphql-url": "^2.0.4",
    "handlebars": "4.0.8",
    "hbs": "^4.0.1",
    "helmet": "^3.9.0",
    "lodash": "^4.17.4",
    "markdown-it": "^8.4.0",
    "node-fetch": "^1.7.3",
    "openid-client": "^1.18.0",
    "passport": "^0.4.0",
    "serve-favicon": "^2.4.5",
    "tagged-template-noop": "^2.1.0"
  },
  "devDependencies": {
    "@dosomething/eslint-config": "^3.1.2",
    "eslint": "^4.12.1",
    "eslint-plugin-prettier": "^2.3.1",
    "nodemon": "^1.12.1",
    "prettier": "1.8.2",
    "prettier-check": "^2.0.0"
  }
}
